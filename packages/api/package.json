{
  "name": "@crtr/api",
  "version": "0.1.0",
  "private": true,
  "description": "CRTR Studio API - Backend services",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "worker": "tsx watch src/worker.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "db:studio": "prisma studio",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:generate": "prisma generate",
    "db:seed": "tsx prisma/seed.ts",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@fal-ai/serverless-client": "^0.14.3",
    "@prisma/client": "^5.22.0",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.66.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.22.1",
    "express-async-errors": "^3.1.1",
    "helmet": "^7.2.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.25",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.19.27",
    "prisma": "^5.22.0",
    "tsx": "^4.21.0",
    "typescript": "^5.3.3"
  }
}
